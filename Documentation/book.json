{
  "title": "Hytale Modding Documentation",
  "description": "Comprehensive guide to creating mods for Hytale using Packs and Plugins",
  "author": "Hytale Modding Community",
  "language": "en",
  "gitbook": ">=3.2.0",
  "plugins": [
    "edit-link",
    "prism",
    "-highlight",
    "github",
    "anchorjs",
    "expandable-chapters-small",
    "back-to-top-button",
    "search-pro",
    "-search",
    "-lunr"
  ],
  "pluginsConfig": {
    "edit-link": {
      "base": "https://github.com/your-username/hytale-modding/edit/main/Documentation",
      "label": "Edit This Page"
    },
    "github": {
      "url": "https://github.com/your-username/hytale-modding"
    },
    "prism": {
      "css": [
        "prism-themes/themes/prism-ghcolors.css"
      ]
    }
  },
  "styles": {
    "website": "styles/website.css"
  },
  "links": {
    "sidebar": {
      "Hytale Official": "https://hytale.com/",
      "Blockbench": "https://www.blockbench.net/",
      "GitHub Repository": "https://github.com/your-username/hytale-modding"
    }
  }
}
